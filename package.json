{
  "name": "evently-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test sequelize db:migrate:undo:all && npm run db:migrate:test && npm run db:migrate:seed:test && NODE_ENV=test nyc --reporter=html --reporter=text mocha --require @babel/register ./src/tests/* --timeout 50000 --exit",
    "start": "node ./build/index.js",
    "dev": "NODE_ENV=development && nodemon --require @babel/register ./src/index.js",
    "build": "rm -rf build && babel -d ./build ./src -s",
    "start:prod": "npm run build && npm run db:migrate && node --require @babel/register ./build/index.js",
    "db:migrate": "./node_modules/.bin/sequelize db:migrate",
    "db:seed": "./node_modules/.bin/sequelize db:seed:all",
    "db:seed:undo": "./node_modules/.bin/sequelize db:seed:undo:all",
    "db:migrate:undo:all": "./node_modules/.bin/sequelize db:migrate:undo:all",
    "db:migrate:test": "NODE_ENV=test ./node_modules/.bin/sequelize db:migrate",
    "db:migrate:seed:test": "NODE_ENV=test ./node_modules/.bin/sequelize db:seed:all",
    "db:migrate:undo:all:test": "NODE_ENV=test ./node_modules/.bin/sequelize db:migrate:undo:all",
    "coverage": "nyc report --reporter=text-lcov && codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getaplotdev/evently-backend.git"
  },
  "husky": {
    "hooks": {
      "pre-push": "set CI=true&&npm test"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ]
  },
