{
  "name": "evently-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test sequelize db:migrate:undo:all && npm run db:migrate:test && npm run db:migrate:seed:test && NODE_ENV=test nyc --reporter=html --reporter=text mocha --require babel-register tests/* --exit",
    "start": "npm run db:migrate && npm run db:seed:undo && npm run db:seed && --require babel-register index.js",
    "dev": "NODE_ENV=development nodemon --require babel-register index.js",
    "lint": "eslint . --fix",
    "db:migrate": "./node_modules/.bin/sequelize db:migrate",
    "db:seed": "./node_modules/.bin/sequelize db:seed:all",
    "db:seed:undo": "./node_modules/.bin/sequelize db:seed:undo:all",
    "db:migrate:undo:all": "./node_modules/.bin/sequelize db:migrate:undo:all",
    "db:migrate:test": "NODE_ENV=test ./node_modules/.bin/sequelize db:migrate",
    "db:migrate:seed:test": "NODE_ENV=test ./node_modules/.bin/sequelize db:seed:all",
    "db:migrate:undo:all:test": "NODE_ENV=test ./node_modules/.bin/sequelize db:migrate:undo:all",
    "coverage": "nyc report --reporter=text-lcov && codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getaplotdev/evently-backend.git"
  },
  "husky": {
    "hooks": {
      "pre-push": "set CI=true&&npm test"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ]
  },
  "author": "getaplotdev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/getaplotdev/evently-backend/issues"
  },
  "homepage": "https://github.com/getaplotdev/evently-backend#readme",
